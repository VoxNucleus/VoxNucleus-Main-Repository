<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
    <servlet>
        <servlet-name>lastEntries</servlet-name>
        <servlet-class>FallingDown.request.lastentries.LastEntries</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BestIn24H</servlet-name>
        <servlet-class>FallingDown.request.topbydate.BestIn24H</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BestIn1Week</servlet-name>
        <servlet-class>FallingDown.request.topbydate.BestIn1Week</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BestIn1Month</servlet-name>
        <servlet-class>FallingDown.request.topbydate.BestIn1Month</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BestIn1Year</servlet-name>
        <servlet-class>FallingDown.request.topbydate.BestIn1Year</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetCommentByDate</servlet-name>
        <servlet-class>FallingDown.request.comment.GetCommentByDate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetCommentByVote</servlet-name>
        <servlet-class>FallingDown.request.comment.GetCommentByVote</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getPostByUser</servlet-name>
        <servlet-class>FallingDown.request.user.getPostByUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Vote</servlet-name>
        <servlet-class>FallingDown.request.vote.vote</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>interestingentries</servlet-name>
        <servlet-class>FallingDown.request.interestingpost.InterestingEntries</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VotePopUp</servlet-name>
        <servlet-class>FallingDown.request.vote.popup.VotePopUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VoteComment</servlet-name>
        <servlet-class>FallingDown.request.vote.comment.VoteComment</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>lastEntries</servlet-name>
        <url-pattern>/lastEntries</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BestIn24H</servlet-name>
        <url-pattern>/bestin24h</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BestIn1Week</servlet-name>
        <url-pattern>/bestin1week</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BestIn1Month</servlet-name>
        <url-pattern>/bestin1month</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BestIn1Year</servlet-name>
        <url-pattern>/bestin1year</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetCommentByDate</servlet-name>
        <url-pattern>/getcommentbydate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetCommentByVote</servlet-name>
        <url-pattern>/getcommentbyvote</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getPostByUser</servlet-name>
        <url-pattern>/getpostbyuser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Vote</servlet-name>
        <url-pattern>/vote</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>interestingentries</servlet-name>
        <url-pattern>/interestingentries</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VotePopUp</servlet-name>
        <url-pattern>/popup/votepopup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VoteComment</servlet-name>
        <url-pattern>/vote/comment</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
